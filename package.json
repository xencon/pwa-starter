{
  "name": "pwa-starter",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "dev-server": "vite --open",
    "dev": "npm run dev-server",
    "dev-task": "vite",
    "deploy": "npx @azure/static-web-apps-cli login --no-use-keychain && npx @azure/static-web-apps-cli deploy",
    "build": "nx exec -- yarn _build ",
    "start": "npm run dev",
    "start-remote": "vite --host",
    "_build": "tsc && vite build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@shoelace-style/shoelace": "^2.4.0",
    "@thepassle/app-tools": "^0.9.9",
    "@types/react": "^17.0.75",
    "lit": "^2.7.2",
    "urlpattern-polyfill": "^7.0.0",
    "workbox-build": "^6.5.2",
    "workbox-core": "^6.5.2",
    "workbox-precaching": "^6.5.2",
    "workbox-window": "^6.6.1",
    "wrap-ansi": "^7.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.3.1",
    "@nx/cypress": "^17.2.8",
    "@playwright/test": "^1.41.1",
    "@types/node": "^16",
    "@wdio/cli": "^8.29.1",
    "@wdio/local-runner": "^8.29.2",
    "@wdio/spec-reporter": "^8.29.1",
    "nx": "17.2.8",
    "rimraf": "^5.0.5",
    "typescript": "^5.0.4",
    "vite": "^4.5.2",
    "vite-plugin-pwa": "^0.14.7"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "bracketSpacing": true
  },
  "nx": {
    "targets": {}
  }
}
