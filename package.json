{
  "name": "pwa-starter",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "dev-server": "vite --open",
    "dev": "npm run dev-server",
    "dev-task": "vite",
    "deploy": "npx @azure/static-web-apps-cli login --no-use-keychain && npx @azure/static-web-apps-cli deploy",
    "build": "nx exec -- yarn _build ",
    "start": "npm run dev",
    "start-remote": "vite --host",
    "_build": "tsc && vite build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@shoelace-style/shoelace": "^2.13.1",
    "@thepassle/app-tools": "^0.9.9",
    "@types/react": "^18.2.48",
    "lit": "^3.1.1",
    "urlpattern-polyfill": "^10.0.0",
    "workbox-build": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-window": "^7.0.0",
    "wrap-ansi": "^9.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.3.1",
    "@nx/cypress": "^17.3.0",
    "@playwright/test": "^1.41.1",
    "@types/node": "^20.11.10",
    "@wdio/cli": "^8.29.1",
    "@wdio/local-runner": "^8.29.2",
    "@wdio/spec-reporter": "^8.29.1",
    "nx": "17.3.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-pwa": "^0.17.5"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "bracketSpacing": true
  },
  "nx": {
    "targets": {}
  }
}
